package projectPractice;

public class login {
	public void Check(String id,String pass)
	{
		if(id =="Mahi_1997") {
			System.out.println("login succcess full");
			return;
		}
		else
		{
			System.out.println("invalid");
		}
		if(pass=="Mahi@1997") {
		System.out.println("login is successful");
		return;
		}
		else 
			System.out.println("invalid");
	}
}
