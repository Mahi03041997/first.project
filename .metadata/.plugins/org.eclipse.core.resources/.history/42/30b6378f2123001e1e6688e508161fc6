package javapractice;

public class MissingNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int arr[] = {1,2,3,4,6,7};

int sum = 0;
for(int i = 0;i<arr.length;i++) {
	sum = sum + arr[i];
}
//System.out.println(sum);
int arr1 = arr.length;
for(int j = 0;j<arr1;j++) {
	sum = sum+arr[j];
}
System.out.println(sum);
	}

}
