package projectPractice;

public class login {
	public void Check(String id,String pass)
	{
		if(id =="Mahi_1997") {
			System.out.println("enter the password : ");
			if(pass=="Mahi@1997")
			{
				System.out.println("login successfully");
			}
			else
			System.out.println("invalid");
			return;
		}
		else
		{
			System.out.println("invalid");
			return;
		}
	}
}
